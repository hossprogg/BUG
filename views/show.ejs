<h2><%= author.name%></h2>
<a href="/authors/<%=author.id%>/edit">Edit</a>
<%-include('./partials/deleteform.ejs', { url : `/author/${author.id}` })%>
<% if(booksByAuthor.length >0 ) {   %>
    <% booksByAuthor.forEach(book =>{ %>
    <a href="/books/<% = book.id %> ">
    <img src="<%book.coverImage%>" alt="coverImage" height="150" width="100">
    </a>
    <% }) %> 
<% }%>